{
  "expo": {
    "name": "EyyTrike",
    "slug": "EyyTrike",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "eyytrike",
    "owner": "Hypoxic",
    "splash": {
      "resizeMode": "contain",
      "image": "./assets/images/EyytrikeLogo2.png",
      "backgroundColor": "#0d4217"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/images/EyytrikeLogo2.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.eyytrike",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to provide navigation and location-based services.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to location to provide navigation and location-based services.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0B4619",
        "foregroundImage": "./assets/images/EyytrikeLogo2.png"
      },
      "package": "com.anonymous.eyytrike",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE",
        "CAMERA",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_ANDROID_GOOGLE_MAPS_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/images/EyytrikeLogo2.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow EyyTrike to use your location to provide navigation and location-based services."
        }
      ],
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "extra": {
      "serverUrl": "https://eyyback.vercel.app",
      "apiUrl": "https://eyyback.vercel.app/api",
      "socketUrl": "https://eyyback.vercel.app",
      "googleMapsApiKey": "YOUR_GOOGLE_MAPS_API_KEY",
      "fallbackUrls": [
        "https://eyyback.vercel.app",
        "http://127.0.0.1:3000"
      ],
      "mongodbUri": "mongodb://localhost:27017/eyytrike",
      "jwtSecret": "your-super-secret-jwt-key-here",
      "router": {},

      "eas": {
        "projectId": "8a2ee4c3-48a6-4251-8db7-5e1e98e36726"
      }
    }
  }
}
